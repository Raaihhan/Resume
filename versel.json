{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "routes": [
      { "handle": "filesystem" },
      { "src": "/resume", "dest": "/index.html" },
      { "src": "/resume/", "dest": "/index.html" },
      { "src": "/resume/(.*)", "dest": "/index.html" }
    ],
    "redirects": [
      { "source": "/", "destination": "/resume/", "permanent": false }
    ]
  }
  