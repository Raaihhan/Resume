{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rewrites": [
      { "source": "/resume", "destination": "/index.html" },
      { "source": "/resume/", "destination": "/index.html" },
      { "source": "/resume/(.*)", "destination": "/index.html" }
    ],
    "redirects": [
      { "source": "/", "destination": "/resume/", "permanent": false }
    ]
  }
  